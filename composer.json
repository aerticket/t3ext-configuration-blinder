{
  "name": "aerticket/configuration-blinder",
  "description": "Hide confidential configuration values from backend users",
  "type": "typo3-cms-extension",
  "version": "2.0.0",
  "license": "GPL-3.0-or-later",
  "keywords": ["typo3", "TYPO3 CMS", "security", "configuration"],
  "require": {
    "php": ">=7.3",
    "typo3/cms-lowlevel": ">=9.5.0,<11.0"
  },
  "autoload": {
    "psr-4": {
      "Aerticket\\ConfigurationBlinder\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "configuration_blinder"
    }
  }
}
